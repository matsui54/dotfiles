{
  "図": {
    "prefix": [
      "zu",
      "ず"
    ],
    "body": "図~\\ref{$1}$2",
    "description": "picture ref"
  },
  "表": {
    "prefix": [
      "hyou",
      "表"
    ],
    "body": "表~\\ref{$1}$2",
    "description": "table ref"
  },
  "コード": {
    "prefix": [
      "code"
    ],
    "body": "コード~\\ref{$1}$2",
    "description": "code ref"
  },
  "Cite": {
      "prefix": "cite",
      "body": ["~\\cite{$1}$2"],
      "description": "Add a cite"
  },
  "setu": {
    "prefix": [
      "setu",
      "せつ"
    ],
    "body": "~\\ref{$1}節$2",
    "description": "setting ref"
  },
  "siki": {
    "prefix": "siki",
    "body": "式\\ref{$1}",
    "description": "equation"
  },
  "si": {
    "prefix": "si",
    "body": "\\si{$1}",
    "description": ""
  },
  "SI": {
    "prefix": "SI",
    "body": "\\SI{$1}{$2}",
    "description": ""
  },
  "slash": {
    "prefix": "\\",
    "body": "\\\\$1{$2}$3",
    "description": ""
  },
  "minipage": {
    "prefix": "minipage",
    "body": [
      "\\begin{figure}[h]",
      "\t\\centering",
      "\t\\begin{minipage}{0.4\\columnwidth}",
      "\t\t\\centering",
      "\t\t\\includegraphics[width=\\columnwidth]{$1}",
      "\t\t\\caption{$2}",
      "\t\t\\label{$3}",
      "\t\\end{minipage}",
      "\t\\begin{minipage}{0.4\\columnwidth}",
      "\t\t\\centering",
      "\t\t\\includegraphics[width=\\columnwidth]{$4}",
      "\t\t\\caption{$5}",
      "\t\t\\label{$6}",
      "\t\\end{minipage}",
      "\\end{figure}"
    ],
    "description": "minipage"
  },
  "subfigure": {
    "prefix": "subfigure",
    "body": [
      "\\begin{figure}[ht]",
      "\t\\centering",
      "\t\\begin{subfigure}{0.45\\columnwidth}",
      "\t\t\\centering",
      "\t\t\\includegraphics[width=0.95\\textwidth]{$3}",
      "\t\t\\caption{$4}",
      "\t\t\\label{fig:$2:$5}",
      "\t\\end{subfigure}",
      "\t\\begin{subfigure}{0.45\\columnwidth}",
      "\t\t\\centering",
      "\t\t\\includegraphics[width=0.95\\textwidth]{$6}",
      "\t\t\\caption{$7}",
      "\t\t\\label{fig:$2:$8}",
      "\t\\end{subfigure}",
      "\t\\caption{$1}",
      "\t\\label{fig:$2}",
      "\\end{figure}"
    ],
    "description": "minipage"
  },
  "fig": {
    "prefix": "fig",
    "body": [
      "\\begin{figure}[h]",
      "\t\\centering",
      "\t\\includegraphics[width=$1\\columnwidth]{$2}",
      "\t\\caption{$3}",
      "\t\\label{$4}",
      "\\end{figure}"
    ],
    "description": "figure"
  },
  "texttt": {
    "prefix": "tt",
    "body": "\\texttt{$1}$2",
    "description": "texttt"
  },
  "dollar": {
    "prefix": "$",
    "body": "\\$ $1 \\$$2",
    "description": "dollar"
  }
}
